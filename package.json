{
  "name": "template-node",
  "version": "0.0.0",
  "description": "template-node",
  "packageManager": "pnpm@8.6.11",
  "type": "module",
  "author": {
    "name": "cola",
    "email": "szmintel@gmail.com",
    "url": "https://aitimi.cn"
  },
  "license": "MIT",
  "funding": "https://github.com/sponsors/szmxx",
  "homepage": "https://github.com/szmxx/template-node#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/szmxx/template-node",
    "directory": "."
  },
  "bugs": {
    "url": "https://github.com/szmxx/template-node/issues"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=14"
  },
  "scripts": {
    "dev": "node --loader ts-node/esm --experimental-specifier-resolution=node --watch-path=src src/index.ts",
    "test": "vitest dev --run",
    "release": "pnpm test && changelogen --release --push && npm publish --access=public",
    "start": "pm2 start ecosystem.config.cjs --env production",
    "prod": "rimraf dist && tsc"
  },
  "keywords": [
    "node"
  ],
  "dependencies": {
    "@koa/cors": "^4.0.0",
    "@koa/router": "^12.0.0",
    "changelogen": "^0.5.5",
    "fast-glob": "^3.3.1",
    "fs-extra": "^11.1.1",
    "joi": "^17.10.1",
    "jsonwebtoken": "^9.0.2",
    "koa": "^2.14.2",
    "koa-body": "^6.0.1",
    "pathe": "^1.1.1",
    "rimraf": "^5.0.1",
    "ts-node": "^10.9.1",
    "typescript": "^5.2.2",
    "vitest": "^0.34.3",
    "winston": "^3.10.0"
  },
  "devDependencies": {
    "@types/fs-extra": "^11.0.1",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/koa": "^2.13.9",
    "@types/koa__cors": "^4.0.1",
    "@types/koa__router": "^12.0.1",
    "@types/node": "^20.5.9",
    "@vercel/node": "^3.0.5",
    "dotenv": "^16.3.1",
    "pm2": "^5.3.0"
  }
}
